@model IEnumerable<Day3MVCProject.Models.CustOrdersOrders_Result>
@{
    ViewBag.Title = "CustomerOrders";
}

<h2>CustomerOrders</h2>

@foreach(var item in Model)
            {
<ul>
    <li>Order id : @item.OrderID</li>
    <li>Order Date : @item.OrderDate</li>
    <li>Required Date : @item.RequiredDate</li>
    <li>Shipped Date : @item.ShippedDate</li>
</ul>
            }